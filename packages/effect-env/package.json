{
  "name": "effect-env",
  "version": "0.5.1",
  "description": "Typed, testable, policy-aware env management for Effect apps",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "environment",
    "variables",
    "effect",
    "schema",
    "typed",
    "hume"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-env"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -b",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "dotenv": "^16.6.1",
    "effect-json": "workspace:*",
    "effect": "^3.19.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "ultracite": "6.3.6",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
