{
  "name": "effect-models",
  "version": "1.0.0",
  "description": "Type-safe Effect services for LLM model providers (OpenRouter, HuggingFace, etc.)",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "effect",
    "llm",
    "openrouter",
    "huggingface",
    "artificial-analysis",
    "models",
    "typescript",
    "hume"
  ],
  "author": "Paul J. Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-models"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "effect": "^3.19.9"
  },
  "peerDependencies": {
    "@effect/platform": "^0.90.10",
    "effect-telemetry": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@effect/platform-node": {
      "optional": true
    },
    "effect-telemetry": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "ultracite": "6.3.6",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
