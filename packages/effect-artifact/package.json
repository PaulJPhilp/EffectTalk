{
  "name": "effect-artifact",
  "version": "0.6.0",
  "description": "Type-safe AI artifact management with versioning, type detection, and rendering hints",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "effect",
    "artifact",
    "ai",
    "code-generation",
    "versioning",
    "type-detection",
    "mermaid",
    "typescript",
    "hume"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@effect/platform": "^0.72.0",
    "effect": "^3.19.14"
  },
  "devDependencies": {
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@biomejs/biome": "2.3.7",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
