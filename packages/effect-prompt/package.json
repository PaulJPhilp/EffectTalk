{
  "name": "effect-prompt",
  "version": "1.0.0",
  "type": "module",
  "description": "Type-safe AI prompt management system with templating, validation, and composition",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "effect": "^3.19.9",
    "effect-env": "workspace:*",
    "effect-json": "workspace:*",
    "effect-liquid": "workspace:*",
    "effect-storage": "workspace:*"
  },
  "peerDependencies": {
    "@effect/platform": "^0.90.10"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "ultracite": "6.3.6",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
