{
  "name": "effect-regex",
  "version": "0.5.2",
  "description": "Type-safe, composable regex builder for TypeScript using Effect",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "regex",
    "regexp",
    "pattern",
    "builder",
    "effect",
    "typescript",
    "type-safe",
    "ast",
    "fluent-api",
    "re2",
    "pcre",
    "mcp",
    "model-context-protocol",
    "security",
    "validation",
    "capture-groups",
    "backreferences",
    "lookahead",
    "lookbehind",
    "hume"
  ],
  "author": "Paul J. Philip",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-regex"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "effect": "^3.19.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/cli": "^0.69.0",
    "@effect/language-service": "latest",
    "@effect/platform": "^0.90.3",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/node": "^20.19.25",
    "@vitest/coverage-v8": "^4.0.3",
    "typescript": "^5.9.3",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
