{
  "name": "effect-mdx",
  "version": "0.5.1",
  "description": "A library for working with MDX using Effect",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "effect",
    "mdx",
    "functional-programming",
    "rehype",
    "remark",
    "unified",
    "mdx-js",
    "hume"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-mdx"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@effect/platform": "^0.90.10",
    "@mdx-js/mdx": "^3.1.0",
    "effect": "^3.19.9",
    "effect-json": "workspace:*",
    "gray-matter": "^4.0.3",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "yaml": "^2.8.1"
  },
  "peerDependencies": {
    "@effect/platform": "^0.90.10"
  },
  "peerDependenciesMeta": {
    "@effect/platform-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@types/unist": "^3.0.3",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "remark-lint": "^10.0.1",
    "typescript": "^5.9.3",
    "ultracite": "6.3.6",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
