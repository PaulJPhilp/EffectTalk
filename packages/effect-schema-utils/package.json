{
  "name": "effect-schema-utils",
  "version": "0.5.2",
  "type": "module",
  "description": "Schema validation utilities for Effect - format and extract information from Effect.Schema parse errors",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "effect",
    "schema",
    "validation",
    "error-handling",
    "utilities"
  ],
  "author": "Paul Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-schema-utils"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "effect": "^3.19.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "typescript": "^5.9.3",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
