{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "~/*": ["src/*"],
      "effect-supermemory/*": ["./src/*"],
      "@/*": ["./src/*"],
      "@services/*": ["./services/*"],
      "@test/*": ["./test/*"]
    },
    "types": ["vitest/globals"],
    "plugins": [
      {
        "name": "@effect/language-service"
      }
    ]
  },
  "include": ["src", "test", "services", "scripts", "vitest.config.ts"]
}
