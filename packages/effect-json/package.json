{
  "name": "effect-json",
  "version": "0.5.1",
  "description": "Type-safe, schema-driven JSON serialization for TypeScript and Effect",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./Toon": {
      "import": "./dist/Toon.js",
      "types": "./dist/Toon.d.ts"
    },
    "./JsonLines": {
      "import": "./dist/JsonLines.js",
      "types": "./dist/JsonLines.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "effect",
    "json",
    "schema",
    "validation",
    "typescript",
    "superjson",
    "jsonc",
    "jsonl",
    "ndjson",
    "serialization",
    "parsing",
    "hume"
  ],
  "author": "Paul J. Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-json"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@toon-format/toon": "^2.0.1",
    "effect": "^3.19.14",
    "effect-schema-utils": "workspace:*"
  },
  "peerDependencies": {
    "superjson": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "superjson": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "superjson": "^2.0.0",
    "typescript": "^5.9.3",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
