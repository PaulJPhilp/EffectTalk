{
  "name": "effect-image",
  "version": "0.6.1",
  "description": "Type-safe, Effect-native image processing and ML preprocessing library",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "keywords": [
    "effect",
    "image",
    "ml",
    "preprocessing",
    "sharp",
    "resize",
    "normalize",
    "tensor",
    "typescript",
    "hume"
  ],
  "author": "Paul J. Philp",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulJPhilp/hume.git",
    "directory": "packages/effect-image"
  },
  "bugs": {
    "url": "https://github.com/PaulJPhilp/hume/issues"
  },
  "homepage": "https://github.com/PaulJPhilp/hume#readme",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome format . --write"
  },
  "dependencies": {
    "@effect/platform": "^0.90.10",
    "effect": "^3.19.9",
    "sharp": "^0.33.5"
  },
  "peerDependencies": {
    "@effect/platform": "^0.90.10"
  },
  "peerDependenciesMeta": {
    "@effect/platform-node": {
      "optional": true
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@effect/platform-node": "^0.96.0",
    "@effect/vitest": "^0.25.1",
    "@types/node": "^20.19.25",
    "@types/sharp": "^0.32.0",
    "typescript": "^5.9.3",
    "ultracite": "^6.5.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "bun": ">=1.0",
    "node": ">=18.18"
  }
}
